{
  "$schema": "https://frontmatter.codes/frontmatter.schema.json",
  "frontMatter.taxonomy.contentTypes": [
    {
      "name": "week",
      "clearEmpty": true,
      "fields": [
        {
          "title": "Title",
          "name": "title",
          "type": "string"
        },
        {
          "title": "Publishing date",
          "name": "date",
          "type": "datetime",
          "default": "{{now}}",
          "isPublishDate": true
        }
      ]
    },
    {
      "name": "writing",
      "clearEmpty": true,
      "fields": [
        {
          "title": "Title",
          "name": "title",
          "type": "string"
        },
        {
          "title": "Description",
          "name": "description",
          "type": "string"
        },
        {
          "title": "Publishing date",
          "name": "date",
          "type": "datetime",
          "default": "{{now}}",
          "isPublishDate": true
        },
        {
          "title": "Tags",
          "name": "content-tags",
          "type": "tags"
        },
        {
          "title": "Categories",
          "name": "categories",
          "type": "categories"
        },
        {
          "title": "Draft",
          "name": "draft",
          "type": "draft"
        }
      ]
    },
    {
      "name": "note",
      "clearEmpty": true,
      "fields": [
        {
          "title": "Date",
          "name": "date",
          "type": "datetime",
          "default": "{{now}}",
          "isPublishDate": true
        },
        {
          "title": "Tags",
          "name": "content-tags",
          "type": "tags"
        },
        {
          "title": "updated",
          "name": "updated",
          "type": "datetime"
        },
        {
          "title": "mp-syndicate-to",
          "name": "mp-syndicate-to",
          "type": "choice",
          "choices": [
            "https://fedi.gerwitz.com/@hans"
          ]
        }
      ]
    },
    {
      "name": "project",
      "fields": [
        {
          "title": "Title",
          "name": "title",
          "type": "string"
        },
        {
          "title": "Description",
          "name": "description",
          "type": "string"
        },
        {
          "title": "Date",
          "name": "date",
          "type": "datetime",
          "isPublishDate": true
        },
        {
          "title": "Tags",
          "name": "content-tags",
          "type": "tags"
        },
        {
          "title": "Draft",
          "name": "draft",
          "type": "draft"
        }
      ]
    },
    {
      "name": "list",
      "fields": [
        {
          "title": "Title",
          "name": "title",
          "type": "string"
        },
        {
          "title": "Description",
          "name": "description",
          "type": "string"
        },
        {
          "title": "Date",
          "name": "date",
          "type": "datetime"
        },
        {
          "title": "Tags",
          "name": "content-tags",
          "type": "tags"
        }
    {
      "name": "default",
      "pageBundle": false,
      "previewPath": "yyyy/MM/dd",
      "slugSuffix": ".html",
      "fields": [
        {
          "title": "Title",
          "name": "title",
          "type": "string"
        },
        {
          "title": "Description",
          "name": "description",
          "type": "string"
        },
        {
          "title": "Publishing date",
          "name": "date",
          "type": "datetime",
          "default": "{{now}}",
          "isPublishDate": true
        },
        {
          "title": "Last modified date",
          "name": "updated",
          "type": "datetime",
          "isModifiedDate": true
        },
        {
          "title": "Content preview",
          "name": "preview",
          "type": "image"
        },
        {
          "title": "Is in draft",
          "name": "draft",
          "type": "draft"
        },
        {
          "title": "Tags",
          "name": "tags",
          "type": "tags"
        },
        {
          "title": "Categories",
          "name": "categories",
          "type": "categories"
        }
      ]
    },
    {
      "name": "week",
      "pageBundle": false,
      "previewPath": "weeks",
      "fields": [
        {
          "title": "Title",
          "name": "title",
          "type": "string"
        },
        {
          "title": "Date",
          "name": "date",
          "type": "datetime"
        },
        {
          "title": "Tags",
          "name": "content-tags",
          "type": "tags"
        }
      ]
    }
  ],
  "frontMatter.framework.id": "11ty",
  "frontMatter.preview.host": "http://localhost:8080",
  "frontMatter.content.pageFolders": [
    {
      "title": "weeks",
      "path": "[[workspace]]/src/weeks",
      "excludeSubdir": false,
      "filePrefix": "{{date|yyyy}}/",
      "contentTypes": [
        "week"
      ]
    },
    {
      "title": "writing",
      "path": "[[workspace]]/src/writing",
      "excludeSubdir": false,
      "filePrefix": "{{date|yyyy}}/{{date|yyyy-MM-dd}}",
      "contentTypes": [
        "writing"
      ]
    },
    {
      "title": "notes",
      "path": "[[workspace]]/src/notes",
      "excludeSubdir": false,
      "filePrefix": "{{date|yyyy}}/{{date|yyyy-MM-dd}}",
      "contentTypes": [
        "note"
      ]
    },
    {
      "title": "projects",
      "path": "[[workspace]]/src/projects",
      "excludeSubdir": false,
      "contentTypes": [
        "project"
      ]
    },
    {
      "title": "lists",
      "path": "[[workspace]]/src/lists",
      "excludeSubdir": false,
      "contentTypes": [
        "list"
      ]
    },
    {
      "title": "logs",
      "path": "[[workspace]]/src/logs",
      "excludeSubdir": false,
      "contentTypes": [
        "log"
      ]
    },
    {
      "title": "media",
      "path": "[[workspace]]/src/media",
      "excludeSubdir": false,
      "contentTypes": [
        "media"
      ]
    }
  ],
  "frontMatter.taxonomy.alignFilename": true,
  "frontMatter.taxonomy.commaSeparatedFields": [
    "content-tags"
  ],
  "frontMatter.dashboard.openOnStart": false,
  "frontMatter.website.host": "https://hans.gerwitz.com",
  "frontMatter.panel.openOnSupportedFile": false
}
